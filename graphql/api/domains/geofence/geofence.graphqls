type Geofence {
	id: Int!
	name: String!
	polygon: GeofenceBounds!
	active: Boolean!
}

type GeofenceEvent {
	id: ID!
	geofence: Geofence!
	track: Track!
	tag: String!
	type: String!
	position: GeoJSONPoint!
	timestamp: DateTimeOffset!
	metadata: JSON
}

type GeofenceBounds {
	polygon: GeoJSONPolygon!
	height: Float
}