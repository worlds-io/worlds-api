input CreateSnapshotInput {
    dataSourceId: ID!
    timestamp: DateTimeOffset!
}

input UploadImageInput {
    dataSourceId: ID
    fileName: String
    timestamp: DateTimeOffset
    data: String!
}

type Image {
    id: ID!
    dataSourceId: ID
    timestamp: DateTimeOffset
    url: String!
    thumbnailUrl: String
}
