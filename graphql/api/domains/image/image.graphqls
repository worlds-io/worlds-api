input CreateSnapshotInput {
    datasourceId: ID!
    timestamp: DateTimeOffset!
}

input UploadImageInput {
    datasourceId: ID
    timestamp: DateTimeOffset
    bytes: [Byte!]!
}

type Image {
    id: ID!
    datasourceId: ID
    timestamp: DateTimeOffset
    url: String!
    thumbnailUrl: String!
}
