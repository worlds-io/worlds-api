type Agent {
	id: ID!
	name: String!
	description: String!
	timezone: String
	active: Boolean!
}

type AgentEvent {
	id: ID!
	agent: Agent!
	type: String!
	subType: String
	startTime: DateTimeOffset!
	endTime: DateTimeOffset
	position: GeoJSONPoint
	timezone: String!
	metadata: JSON
}

input AgentEventInput {
	id: ID!
	agentId: String!
	type: String!
	subType: String
	startTime: DateTimeOffset!
	endTime: DateTimeOffset
	position: GeoJSONPoint
	timezone: String
	metadata: JSON
}