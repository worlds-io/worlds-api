### Text Recognition ###

input TextRecognitionOptionsInput {
    regions: [GeoJSONMultiPolygonInput!]
}

type TextRecognitionResponse {
    results: [TextRecognitionResult!]!
}

type TextRecognitionResult {
    textDetection: TextDetection!
    regionIndex: Int
}

type TextDetection {
    polygon: GeoJSONPolygon!
    text: String!
    confidence: Float
}