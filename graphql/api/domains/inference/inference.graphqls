### Text Detection ###

input TextDetectionOptionsInput {
    regions: [GeoJSONMultipolygon!]
}

type TextDetectionResponse {
    results: [TextDetectionResult!]!
}

type TextDetectionResult {
    textDetection: TextDetection!
    regionIndex: Int
}

type TextDetection {
    polygon: GeoJSONPolygon!
    text: String!
    confidence: Float
}