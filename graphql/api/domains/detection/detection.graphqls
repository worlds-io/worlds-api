type Detection {
	id: ID
	globalTrackId: String
	cameraId: Int!
	tag: String!
	timestamp: DateTimeOffset!
	box: Box
	polygon: String
	position: String
	track: Track
	zoneIds: [Int!]!
	geofenceIds: [Int!]!
	zoneEvents: [ZoneEvent!]!
	geofenceEvents: [GeofenceEvent!]!
	properties: DetectionProperties
	metadata: JSON
	createdAt: DateTimeOffset
	updatedAt: DateTimeOffset
}

type DetectionProperties {
	color: String
	size: Float
	shape: String
	pose: Float
	relationships: JSON
}

