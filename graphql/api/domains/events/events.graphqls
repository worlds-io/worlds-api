type EventProducer {
	id: ID!
	name: String!
	description: String
	timezone: String
	active: Boolean!
	configDetails: JSON
}

input EventProducerInput {
	id: ID
	name: String!
	description: String
	timezone: String
	active: Boolean
	configDetails: JSON
}

type Event {
	id: ID!
	eventProducer: EventProducer!
	type: String!
	subType: String
	startTime: DateTimeOffset!
	endTime: DateTimeOffset
	position: GeoJSONPoint
	timezone: String
	metadata: JSON
	images: [Image!]!
}

input CreateEventInput {
	id: ID
	eventProducerId: String!
	type: String!
	subType: String
	startTime: DateTimeOffset!
	endTime: DateTimeOffset
	position: GeoJSONPointInput
	timezone: String
	metadata: JSON
	snapshots: [CreateSnapshotInput!]
	uploads: [UploadImageInput!]
}

input UpdateEventInput {
	id: ID!
	type: String!
	subType: String
	startTime: DateTimeOffset!
	endTime: DateTimeOffset
	position: GeoJSONPointInput
	timezone: String
	metadata: JSON
	snapshots: [CreateSnapshotInput!]
	uploads: [UploadImageInput!]
}